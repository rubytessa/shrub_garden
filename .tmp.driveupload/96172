\hypertarget{shrub-growth-biomass-increase}{%
\subsection{Shrub growth (biomass
increase)}\label{shrub-growth-biomass-increase}}

\begin{itemize}
\item
  photosynthetic canopy area
\item
  new leaves, leaf size
\item
  height
\item
  new stems
\item
  height relative to competitors
\end{itemize}

\hypertarget{neighbors}{%
\subsection{Neighbors}\label{neighbors}}

\begin{itemize}
\tightlist
\item
  species ID of neighbors
\item
  height and density of surrounding neighbors
\end{itemize}

\hypertarget{environmental-neighborhood-of-shrublings}{%
\subsection{Environmental Neighborhood of
Shrublings}\label{environmental-neighborhood-of-shrublings}}

\begin{itemize}
\tightlist
\item
  temperature, moisture
\item
  light environment
\item
  nutrient availability
\item
  maximum snow depth
\end{itemize}

